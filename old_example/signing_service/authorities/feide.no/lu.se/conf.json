{
  "constraints": {
    "max_path_length": 1
  },
  "metadata_policy": {
    "openid_provider": {
      "contacts": {
        "add": "operations@feide.no"
      },
      "token_endpoint_auth_methods_supported": {
        "subset_of": [
          "client_secret_jwt",
          "private_key_jwt"
        ]
      },
      "token_endpoint_auth_signing_alg_values_supported": {
        "default": "ES256",
        "subset_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      },
      "userinfo_signing_alg_values_supported": {
        "default": "ES256",
        "subset_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      },
      "id_token_signing_alg_values_supported": {
        "default": "ES256",
        "subset_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      }
    },
    "openid_relying_party": {
      "id_token_signed_response_alg": {
        "default": "ES256",
        "one_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      },
      "userinfo_signed_response_alg": {
        "default": "ES256",
        "one_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      },
      "token_endpoint_auth_signing_alg": {
        "default": "ES256",
        "one_of": [
          "ES256",
          "ES384",
          "ES512"
        ]
      }
    }
  }
}