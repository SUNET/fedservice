{
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "root": {
      "handlers": [
        "default",
        "console"
      ],
      "level": "DEBUG"
    },
    "loggers": {
      "entity": {
        "level": "DEBUG"
      }
    },
    "handlers": {
      "default": {
        "class": "logging.FileHandler",
        "filename": "seid/debug.log",
        "formatter": "default"
      },
      "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "default"
      }
    },
    "formatters": {
      "default": {
        "format": "%(asctime)s %(name)s %(levelname)s %(message)s"
      }
    }
  },
  "federation": {
    "entity_id": "https://127.0.0.1:7001",
    "keys": {
      "private_path": "seid/fed_keys.json",
      "key_defs": [
        {
          "type": "RSA",
          "use": [
            "sig"
          ]
        },
        {
          "type": "EC",
          "crv": "P-256",
          "use": [
            "sig"
          ]
        }
      ],
      "public_path": "static/seid_fed_keys.json",
      "read_only": false
    },
    "entity_type": "federation_entity",
    "opponent_entity_type": "federation_entity",
    "endpoint": {
      "fetch": {
        "path": "fetch",
        "class": "fedservice.entity.fetch.Fetch",
        "kwargs": {
          "client_authn_method": null
        }
      },
      "list": {
        "path": "list",
        "class": "fedservice.entity.list.List",
        "kwargs": {
          "client_authn_method": null,
          "subordinates": {
            "class": "idpyoidc.storage.abfile.AbstractFileSystem",
            "kwargs": {
              "fdir": "seid/subordinates",
              "value_conv": "idpyoidc.util.JSON",
              "key_conv": "idpyoidc.util.QPKey"
            }
          }
        }
      }
    }
  },
  "webserver": {
    "server_cert": "certs/example.crt",
    "server_key": "certs/example.key",
    "cert_chain": null,
    "port": 7001,
    "domain": "127.0.0.1",
    "debug": true
  }
}
