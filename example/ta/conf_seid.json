{
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "root": {
      "handlers": [
        "default",
        "console"
      ],
      "level": "DEBUG"
    },
    "loggers": {
      "entity": {
        "level": "DEBUG"
      }
    },
    "handlers": {
      "default": {
        "class": "logging.FileHandler",
        "filename": "log/seid_debug.log",
        "formatter": "default"
      },
      "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "default"
      }
    },
    "formatters": {
      "default": {
        "format": "%(asctime)s %(name)s %(levelname)s %(message)s"
      }
    }
  },
  "entity": {
    "entity_id": "https://127.0.0.1:7001",
    "key_config": {
      "private_path": "private/fed_seid_keys.json",
      "key_defs": [
        {
          "type": "RSA",
          "use": [
            "sig"
          ]
        },
        {
          "type": "EC",
          "crv": "P-256",
          "use": [
            "sig"
          ]
        }
      ],
      "public_path": "static/seid_fed_keys.json",
      "read_only": false
    },
    "subordinate": "file:ta/seid_subordinates.json",
    "endpoints": [
      "entity_configuration",
      "fetch",
      "list"
    ]
  },
  "webserver": {
    "server_cert": "certs/example.crt",
    "server_key": "certs/example.key",
    "cert_chain": null,
    "port": 7001,
    "domain": "127.0.0.1",
    "debug": true
  }
}
