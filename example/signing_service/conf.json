{
  "logging": {
    "version": 1,
    "root": {
      "handlers": [
        "default",
        "console"
      ],
      "level": "DEBUG"
    },
    "loggers": {
      "idp": {
        "level": "DEBUG"
      }
    },
    "handlers": {
      "default": {
        "class": "logging.FileHandler",
        "filename": "debug.log",
        "formatter": "default"
      },
      "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "default"
      }
    },
    "formatters": {
      "default": {
        "format": "%(asctime)s %(name)s %(levelname)s %(message)s"
      }
    }
  },
  "port": 6000,
  "domain": "127.0.0.1",
  "keydefs": [
    {
      "type": "RSA",
      "key": "",
      "use": [
        "sig"
      ]
    },
    {
      "type": "EC",
      "crv": "P-256",
      "use": [
        "sig"
      ]
    }
  ],
  "server_info": {
    "class": "fedservice.metadata_api.fsd.FSEntityStatementAPIMulti",
    "kwargs": {
      "federation_api_endpoint": "https://{domain}:{port}/api/{{}}",
      "entity_id_pattern": "https://{domain}:{port}/eid/{{}}",
      "federation_entities": "federation_entity",
      "authorities": "authorities",
      "lifetime": 3600
    }
  },
  "httpc_params": {
    "verify_ssl": false
  },
  "webserver": {
    "server_cert": "certs/example.crt",
    "server_key": "certs/example.key",
    "port": 6000,
    "domain": "127.0.0.1"
  }
}
